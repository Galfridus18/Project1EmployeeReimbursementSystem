<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>LoginEmployee</servlet-name>
    <servlet-class>LoginEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginEmployee</servlet-name>
    <url-pattern>/LoginEmployee</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginManager</servlet-name>
    <servlet-class>LoginManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginManager</servlet-name>
    <url-pattern>/LoginManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeLogout</servlet-name>
    <servlet-class>EmployeeLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeLogout</servlet-name>
    <url-pattern>/EmployeeLogout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ManagerLogout</servlet-name>
    <servlet-class>ManagerLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerLogout</servlet-name>
    <url-pattern>/ManagerLogout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeServlet</servlet-name>
    <servlet-class>EmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeServlet</servlet-name>
    <url-pattern>/EmployeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
